        for i in string[-1:0:-1]+string[0]:
            if i == "A" or i == "a":
                sub += "@"
                cont += 1
            elif i == "E" or i == "e":
                sub += "3"
                cont += 1
            elif i == "I" or i == "i":
                sub += "1"
                cont += 1
            elif i == "O" or i == "o":
                sub += "0"
                cont += 1
            elif i == "T" or i == "t":
                sub += "7"
                cont += 1
            elif i == "S" or i == "s":
                sub += "5"
                cont += 1
            else:
                sub += i
        print(sub)
        print(cont)


